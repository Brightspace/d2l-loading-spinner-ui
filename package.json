{
  "description": "Polymer-based web component for a D2L loading spinner",
  "keywords": [
    "d2l",
    "polymer",
    "loading"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BrightspaceUI/loading-spinner.git"
  },
  "name": "d2l-loading-spinner",
  "scripts": {
    "lint": "npm run lint:wc && npm run lint:js",
